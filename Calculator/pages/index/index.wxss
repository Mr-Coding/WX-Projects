page{
  height: 100%; 
  background-color: #e0d9c7;
  display: flex;
  justify-content: center;
}

 .container{
  width: 90%;
  /* height: 100%;  */
  padding-top: 50rpx;
  /* display: flex; */
  /* flex-flow: column;  */
  /* justify-content: center; */
  background-color: #e0d9c7; 
} 

.show-panel{
  /* width: 90%; */
  height: 200rpx;
  padding-top: 15rpx;
  padding-right: 15rpx;
  padding-left: 15rpx;
  border: 2px #111 solid;
  border-radius: 5px ;
  background-color: #f7f5c2;
   font-family: DS-Digital; 
}

.expression{
  height: 80rpx;
  overflow: auto;  
  white-space: nowrap;
  display: block;
  text-align: right;
  font-size: 70rpx;
}
.inoutNumbre{
  height: 100rpx;
  overflow: auto;  
  white-space: nowrap;
  display: block;
  text-align: right;
  font-size: 90rpx;
}

/*------按钮区------*/
.button-panel{
  /* width: 90%; */
  height: 800rpx;
  margin-top: 55rpx;
  /* padding: 30rpx;  */
}

.row-1,.row-2,.row-3,.row-4 {
  display: flex;
  justify-content: center;
}
.row-4-left{
  display: flex;
  flex-direction: column;
}

.btn {
  margin: 7rpx;
  text-align: center;
  border-radius: 6px;
  font-family: "微软雅黑";
	font-weight: bold;  
}

.square{
  width: 150rpx; 
  height: 150rpx;
  line-height: 150rpx;
}
.rectangle{
  width: 150rpx;
  line-height: 300rpx;
}
.rectangle-r{
  width: 308rpx;
   /* height: 150rpx;  */
   line-height: 150rpx; 
}

/*------颜色------*/
.Theme-color{
  background-color: #e0d9c7;
}

.blue{
	background-color: #2585f6;
  color: #fff;
}
.blue-active{
  background-color: #2172d1;
}

.red{
	background-color: #ee3d47;
  color: #fff;
}
.red-active{
	background-color: #b12129;  
}

.orange{
  background-color: #F6B83D;
  color: #fff;    
}
.orange-active{
  background-color: #CA9730;
}

.gray{
	background-color: #717173;
  color: #fff;  
}
.gray-active{
  background-color:#494949;
}

.white{
	background-color: #FFFFFF;
  color: #000;
}
.white-active{
  background-color:#bebebe;
}

/*------字体------*/
@font-face {
	font-family: 'DS-Digital';
	src: url('DS-Digital-BoldItalic.eot');
	font-weight: bold;
	font-style: italic;
}
@font-face {
	font-family: 'DS-Digital';
	src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
		url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
		url('DS-Digital-BoldItalic.ttf') format('truetype'),
		url('DS-Digital-BoldItalic.svg#DS-Digital-BoldItalic') format('svg');
	font-weight: bold;
	font-style: italic;
}

